{
	"name": "praetor",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"component": "node ./.scripts/new-component.js",
		"postinstall": "paraglide-js compile --project ./project.inlang",
		"gh-pages": "npm run build && npx gh-pages -d build"
	},
	"devDependencies": {
		"@inlang/paraglide-js": "1.2.1",
		"@inlang/paraglide-js-adapter-sveltekit": "^0.1.1",
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/adapter-static": "^3.0.1",
		"@sveltejs/kit": "^2.4.1",
		"@sveltejs/vite-plugin-svelte": "^3.0.0",
		"@types/eslint": "8.56.0",
		"@types/gh-pages": "^6",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"enquirer": "^2.4.1",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"gh-pages": "^6.1.1",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"sass": "^1.69.7",
		"svelte": "^4.2.7",
		"svelte-check": "^3.6.0",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"type": "module",
	"dependencies": {
		"@stdlib/ndarray": "^0.1.1",
		"dexie": "^3.2.4",
		"lucide-svelte": "^0.309.0",
		"overlayscrollbars": "^2.4.6",
		"overlayscrollbars-svelte": "^0.5.2",
		"resize-observer-polyfill": "^1.5.1",
		"simplebar": "^6.2.5",
		"zod": "^3.22.4"
	},
	"packageManager": "yarn@4.0.2"
}
